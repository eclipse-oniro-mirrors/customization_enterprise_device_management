        "netmanager_ext",
        "hilog",
        "hisysevent",
        "init",
        "ipc",
        "napi",
        "os_account",
        "relational_store",
        "safwk",
        "samgr",
        "time_service",
        "update_service",
        "wifi",
        "eventhandler",
        "screenlock_mgr"
      ],
      "third_party": [
        "bounds_checking_function",
        "jsoncpp"
      ]
    },
    "build": {
      "group_type":{
        "fwk_group":[
          "//base/customization/enterprise_device_management/interfaces/kits/account_manager:accountmanager",
